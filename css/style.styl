.brand-logo
  font-family Violetta
.nav-text
  transition: background-color .3s;
  font-size: 1.4rem;
  color: #fff;
  display: block;
  padding: 0 40px;
  cursor: pointer;
.about-header
  display: flex;
  justify-content: center;
  align-items: center;
  font-family: Blackhawk;
  color: rgb(167, 2, 151);
.about-text
  font-family: 'Open Sans Condensed', sans-serif
  color:black
.about
  font-size 100px
.about-p1
  font-size: 25px;
  padding: 10px;
  background aliceblue
.about-p2
  font-size: 25px;
  padding: 5px;
  background: #ddd3f5;
.about-p3
  font-size: 25px;
  padding: 5px;
  background: #c3fbfb;
.port-header
  font-family: Blackhawk;
  font-size: 140px;
  color: rgb(31, 193, 234);
  display: flex;
  justify-content: flex-start;
  align-items: center;
.port-text
  background: rgba(240, 248, 255, 0.94);
  font-size: 20px;
  font-family: 'Open Sans Condensed', sans-serif;
.port-card
  width 350px
.port
  font-size: 80px;
  padding-left: 10px;

.built
  font-size: 20px;
  font-family: 'Open Sans Condensed', sans-serif;
  background: #6ccfd2;
  padding: 5px;
.skill-row
  display: flex;
  justify-content: flex-start;
  align-items: baseline;
  font-size: 25px;
  flex: .33;
.skill-name
  font-family: 'Open Sans Condensed', sans-serif;
  padding: 10px;
  background: rgba(240, 255, 255, 0.91);
.skills
  display flex
.starRow
  color rgb(85, 43, 236);
.contact-header
  display flex
  flex-direction column
#contactForm
  padding: 100px;
  background: rgba(240, 248, 255, 0.93);

  .input-field label {
    color: #0c0c0c;
    font-family: 'Open Sans Condensed', sans-serif;
    padding: 2px;
    background: #84f3f3;
    font-size 16px
  }

  .input-field input[type=text]:focus + label {
    color: #0c0c0c;
    font-family: 'Open Sans Condensed', sans-serif;
    padding: 2px;
    background: #84f3f3;
    font-size 16px
  }

  .input-field input[type=text]:focus {
    border-bottom: 1px solid #961cde;
    box-shadow: 0 1px 0 0 #000;
  }

  .materialize-textarea:focus {
    border-bottom: 1px solid #961cde;
    box-shadow: 0 1px 0 0 #000;
  }

  .materialize-textarea:focus + label {
    color: #0c0c0c;
    font-family: 'Open Sans Condensed', sans-serif;
    padding: 2px;
    background: #84f3f3;
    font-size 16px
  }
 .con
    font-family: Blackhawk;
    padding-left: 20px;
    color: rgba(84, 123, 156, 0.91);
    font-size: 100px;
