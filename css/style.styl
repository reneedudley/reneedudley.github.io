#index-banner
  height 100vh
  overflow inherit;
#nav
  background-color: rgba(10, 10, 10, 0.67);
.nav-links
  font-family: 'Just Me Again Down Here', cursive;
.polariod
  height:250px

#folder
  font-size: 140px
  -ms-transform: rotate(7deg) /* IE 9 */
  -webkit-transform: rotate(7deg) /* Chrome, Safari, Opera */
  transform: rotate(7deg)
  color rgba(100, 172, 234, 0.87)

@font-face
  font-family: 'Blackhawk';
  src: url('../css/fonts/Blackhawk/BLACKHAWK.eot');
  src: url('../css/fonts/Blackhawk/BLACKHAWK.eot?#iefix') format('embedded-opentype'),
          url('../css/fonts/Blackhawk/BLACKHAWK.woff') format('woff'),
          url('../css/fonts/Blackhawk/BLACKHAWK.ttf') format('truetype'),
          url('../css/fonts/Blackhawk/BLACKHAWK.svg#BLACKHAWK') format('svg');

@font-face
  font-family: 'Violetta';
  src: url('../css/fonts/Script/Violetta Script.eot');
  src: url('../css/fonts/Script/Violetta Script.eot?#iefix') format('embedded-opentype'),
          url('../css/fonts/Script/Violetta Script.woff') format('woff'),
          url('../css/fonts/Script/Violetta Script.ttf') format('truetype'),
          url('../css/fonts/Script/Violetta Script.svg#Violetta Script') format('svg');


.greeting
  font-family: 'Blackhawk';
  font-size: 200px;
  color: #7d48ad;
.home-background
  display: flex;
  flex-direction: column;
  align-items: center;
.tagline
